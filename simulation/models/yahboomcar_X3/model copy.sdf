     ********** 差速驱动插件：适配原始车轮位姿 **********
    <plugin name="gazebo_ros_diff_drive" filename="libgazebo_ros_diff_drive.so">
      <left_joint>back_left_joint</left_joint>
      <right_joint>back_right_joint</right_joint>
      <front_left_joint>front_left_joint</front_left_joint>
      <front_right_joint>front_right_joint</front_right_joint>
      
      <wheel_separation>0.169</wheel_separation>  <!-- 匹配原始轮距（0.0845*2） -->
      <wheel_diameter>0.08</wheel_diameter>       <!-- 车轮直径（根据STL实际尺寸） -->
      <wheel_acceleration>1.0</wheel_acceleration>
      <wheel_torque>5.0</wheel_torque>
      
      <command_topic>cmd_vel</command_topic>
      <odometry_topic>odom</odometry_topic>
      <odometry_frame>odom</odometry_frame>
      <robot_base_frame>base_footprint</robot_base_frame>
      <publish_odometry>true</publish_odometry>
      <publish_tf>true</publish_tf>
    </plugin>

    <!-- ********** 语音交互插件（预留接口） **********
    <plugin name="dummy_voice_plugin" filename="libgazebo_ros_utils.so">
      <ros>
        <namespace>/yahboomcar</namespace>
        <remapping>voice_cmd:=voice/command</remapping>
      </ros>
    </plugin> -->

