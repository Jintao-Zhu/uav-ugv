      <inertial>
        <pose>2e-06 -1e-06 0.000641 0 -0 0</pose>
        <mass>0.051543</mass>
        <inertia>
          <ixx>1.4597e-05</ixx>
          <ixy>-4.7945e-10</ixy>
          <ixz>-2.4786e-10</ixz>
          <iyy>1.4598e-05</iyy>
