      </axis>
    </joint>

    <link name='front_right_wheel'>
      <pose relative_to='front_right_joint'>0 0 0 0 -0 0</pose>  <!-- 原始位姿不变 -->
      <inertial>
        <pose>2e-06 -0 -0.000641 0 -0 0</pose>
        <mass>0.051532</mass>
        <inertia>
          <ixx>1.4595e-05</ixx>
          <ixy>-4.3975e-10</ixy>
          <ixz>4.415e-10</ixz>
          <iyy>1.4594e-05</iyy>
          <iyz>-1.7476e-09</iyz>
          <izz>2.4264e-05</izz>
        </inertia>
