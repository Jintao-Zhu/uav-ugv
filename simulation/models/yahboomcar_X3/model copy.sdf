<?xml version="1.0" ?> <!--加了传感器参数和动力插件之后轮子消失了-->
<sdf version='1.7'>
  <model name='yahboomcar'>
    <!-- 补充模型基础配置 -->
    <static>false</static>  <!-- 动态模型，允许运动 -->
    <allow_auto_disable>true</allow_auto_disable>

    <link name='base_footprint'>
      <inertial>
        <pose>0.00687 0.000117 0.082436 0 -0 0</pose>
        <mass>0.53461</mass>
        <inertia>
          <ixx>0.00289164</ixx>
          <ixy>-7.86213e-07</ixy>
          <ixz>8.08187e-06</ixz>
          <iyy>0.00554364</iyy>
          <iyz>-3.63694e-06</iyz>
          <izz>0.00500707</izz>
        </inertia>
      </inertial>

      <!-- ********** 碰撞属性补充：物理接触表面参数 ********** -->
      <collision name='base_footprint_fixed_joint_lump__base_link_collision'>
        <pose>0 0 0.0815 0 -0 0</pose>
        <geometry>
          <mesh>
            <scale>1 1 1</scale>
            <uri>model://yahboomcar_X3/meshes/base_link_X3.STL</uri>
          </mesh>
        </geometry>
        <surface>
          <friction>
            <ode>
              <mu>1.0</mu>        <!-- 主摩擦系数 -->
              <mu2>0.8</mu2>      <!-- 次摩擦系数 -->
              <fdir1>0 1 0</fdir1><!-- 摩擦方向 -->
            </ode>
          </friction>
          <contact>
            <ode>
              <kp>1e5</kp>        <!-- 接触刚度 -->
              <kd>1e3</kd>        <!-- 接触阻尼 -->
            </ode>
          </contact>
        </surface>
      </collision>

      <collision name='base_footprint_fixed_joint_lump__camera_link_collision_1'>
        <pose>0.057105 1.8e-05 0.11905 0 -0 0</pose>
        <geometry>
          <mesh>
            <scale>1 1 1</scale>
            <uri>model://yahboomcar_X3/meshes/camera_link.STL</uri>
          </mesh>
        </geometry>
        <surface>
          <friction>
            <ode>
              <mu>0.5</mu>        <!-- 相机碰撞摩擦 -->
              <mu2>0.3</mu2>
            </ode>
          </friction>
        </surface>
      </collision>

      <collision name='base_footprint_fixed_joint_lump__laser_link_collision_2'>
        <pose>0.0435 5.3e-05 0.1915 0 -0 0</pose>
        <geometry>
          <mesh>
            <scale>1 1 1</scale>
            <uri>model://yahboomcar_X3/meshes/laser_link.STL</uri>
          </mesh>
        </geometry>
        <surface>
          <friction>
            <ode>
              <mu>0.6</mu>        <!-- 雷达碰撞摩擦 -->
              <mu2>0.4</mu2>
            </ode>
          </friction>
        </surface>
      </collision>

      <!-- 可视化：完全保留原始配置，确保底盘/传感器显示正常 -->
      <visual name='base_footprint_fixed_joint_lump__base_link_visual'>
        <pose>0 0 0.0815 0 -0 0</pose>
        <geometry>
          <mesh>
            <scale>1 1 1</scale>
            <uri>model://yahboomcar_X3/meshes/base_link_X3.STL</uri>
          </mesh>
        </geometry>
        <material>
          <ambient>0.3 0.3 0.3 1</ambient>
          <diffuse>0 0.7 0 1</diffuse>
          <specular>0.1 0.1 0.1 1</specular>
          <emissive>0 0 0 1</emissive>
        </material>
      </visual>

      <visual name='base_footprint_fixed_joint_lump__camera_link_visual_1'>
        <pose>0.057105 1.8e-05 0.11905 0 -0 0</pose>
        <geometry>
          <mesh>
            <scale>1 1 1</scale>
            <uri>model://yahboomcar_X3/meshes/camera_link.STL</uri>
          </mesh>
        </geometry>
        <material>
          <ambient>0.3 0.3 0.3 1</ambient>
          <diffuse>0.7 0.7 0.7 1</diffuse>
          <specular>0.1 0.1 0.1 1</specular>
          <emissive>0 0 0 1</emissive>
        </material>
      </visual>

      <visual name='base_footprint_fixed_joint_lump__laser_link_visual_2'>
        <pose>0.0435 5.3e-05 0.1915 0 -0 0</pose>
        <geometry>
          <mesh>
            <scale>1 1 1</scale>
            <uri>model://yahboomcar_X3/meshes/laser_link.STL</uri>
          </mesh>
        </geometry>
        <material>
          <ambient>0.3 0.3 0.3 1</ambient>
          <diffuse>0.7 0.7 0.7 1</diffuse>
          <specular>0.1 0.1 0.1 1</specular>
          <emissive>0 0 0 1</emissive>
        </material>
      </visual>

      <!-- ********** 传感器插件：思岚 S2L 激光雷达 ********** -->
      <sensor name="rplidar_s2l" type="ray">
        <pose>0.0435 5.3e-05 0.1915 0 -0 0</pose>  <!-- 与原始激光link位姿一致 -->
        <update_rate>10</update_rate>  <!-- 匹配实物8-10Hz -->
        <ray>
          <scan>
            <horizontal>
              <samples>360</samples>
              <resolution>1.0</resolution>
              <min_angle>-3.1416</min_angle>
              <max_angle>3.1416</max_angle>
            </horizontal>
          </scan>
          <range>
            <min>0.05</min>
            <max>18.0</max>
          </range>
          <noise>
            <type>gaussian</type>
            <mean>0.0</mean>
            <stddev>0.01</stddev>
          </noise>
        </ray>
        <plugin name="gazebo_ros_laser" filename="libgazebo_ros_laser.so">
          <ros>
            <namespace>/yahboomcar</namespace>
            <remapping>scan:=scan</remapping>
          </ros>
          <frame_name>laser_link</frame_name>
          <topic_name>scan</topic_name>
        </plugin>
      </sensor>

      <!-- ********** 传感器插件：Astra Pro Plus 深度相机 ********** -->
      <sensor name="astra_pro_plus" type="depth">
        <pose>0.057105 1.8e-05 0.11905 0 -0 0</pose>  <!-- 与原始相机link位姿一致 -->
        <update_rate>30</update_rate>
        <camera name="head">
          <horizontal_fov>1.047</horizontal_fov>  <!-- 60°视场角 -->
          <image>
            <width>640</width>
            <height>480</height>
            <format>R8G8B8</format>
          </image>
          <depth_camera>
            <depth_image>
              <width>640</width>
              <height>480</height>
            </depth_image>
            <range>
              <min>0.1</min>
              <max>8.0</max>
            </range>
          </depth_camera>
        </camera>
        <plugin name="gazebo_ros_openni_kinect" filename="libgazebo_ros_openni_kinect.so">
          <ros>
            <namespace>/yahboomcar</namespace>
            <remapping>rgb/image_raw:=camera/rgb/image_raw</remapping>
            <remapping>depth/image_raw:=camera/depth/image_raw</remapping>
          </ros>
          <frame_name>camera_link</frame_name>
          <depth_image_topic>depth/image_raw</depth_image_topic>
          <rgb_image_topic>rgb/image_raw</rgb_image_topic>
        </plugin>
      </sensor>

    </link>

    <!-- ********** 左后轮：完全保留原始关节位姿，仅补充transmission和阻尼 ********** -->
    <transmission name="back_left_transmission">
      <type>transmission_interface/SimpleTransmission</type>
      <joint name="back_left_joint">
        <hardwareInterface>hardware_interface/VelocityJointInterface</hardwareInterface>
      </joint>
      <actuator name="back_left_motor">
        <hardwareInterface>hardware_interface/VelocityJointInterface</hardwareInterface>
        <mechanicalReduction>30</mechanicalReduction>  <!-- 1:30减速比 -->
      </actuator>
    </transmission>

    <joint name='back_left_joint' type='revolute'>
      <!-- 恢复原始位姿：-0.08 0.084492 0.04196（未修改z轴） -->
      <pose relative_to='base_footprint'>-0.08 0.084492 0.04196 1.5703 -0 0</pose>
      <parent>base_footprint</parent>
      <child>back_left_wheel</child>
      <axis>
        <xyz>0 0 1</xyz>
        <limit>
          <effort>100</effort>
          <velocity>1</velocity>
          <lower>-1e+16</lower>
          <upper>1e+16</upper>
        </limit>
        <dynamics>
          <spring_reference>0</spring_reference>
          <spring_stiffness>0</spring_stiffness>
          <damping>0.02</damping>  <!-- 补充阻尼，优化稳定性 -->
        </dynamics>
      </axis>
    </joint>

    <link name='back_left_wheel'>
      <pose relative_to='back_left_joint'>0 0 0 0 -0 0</pose>  <!-- 原始位姿不变 -->
      <inertial>
        <pose>-0 2e-06 0.000591 0 -0 0</pose>
        <mass>0.0515309</mass>
        <inertia>
          <ixx>1.45943e-05</ixx>
          <ixy>-3.19787e-10</ixy>
          <ixz>1.74943e-09</ixz>
          <iyy>1.45949e-05</iyy>
          <iyz>1.6177e-10</iyz>
          <izz>2.42637e-05</izz>
        </inertia>
      </inertial>
      <collision name='back_left_wheel_collision'>
        <pose>0 0 0 0 -0 0</pose>
        <geometry>
          <mesh>
            <scale>1 1 1</scale>
            <uri>model://yahboomcar_X3/meshes/back_left_wheel_X3.STL</uri>
          </mesh>
        </geometry>
        <surface>
          <friction>
            <ode>
              <mu>1.5</mu>        <!-- 麦克纳姆轮抓地力 -->
              <mu2>1.5</mu2>
            </ode>
          </friction>
        </surface>
      </collision>

      <visual name='back_left_wheel_visual'>
        <pose>0 0 0 0 -0 0</pose>
        <geometry>
          <mesh>
            <scale>1 1 1</scale>
            <uri>model://yahboomcar_X3/meshes/back_left_wheel_X3.STL</uri>
          </mesh>
        </geometry>
        <material>
          <ambient>0.3 0.3 0.3 1</ambient>
          <diffuse>0.7 0.7 0.7 1</diffuse>
          <specular>0.1 0.1 0.1 1</specular>
          <emissive>0 0 0 1</emissive>
        </material>
      </visual>
    </link>

    <!-- ********** 右后轮：完全保留原始关节位姿，仅补充transmission和阻尼 ********** -->
    <transmission name="back_right_transmission">
      <type>transmission_interface/SimpleTransmission</type>
      <joint name="back_right_joint">
        <hardwareInterface>hardware_interface/VelocityJointInterface</hardwareInterface>
      </joint>
      <actuator name="back_right_motor">
        <hardwareInterface>hardware_interface/VelocityJointInterface</hardwareInterface>
        <mechanicalReduction>30</mechanicalReduction>
