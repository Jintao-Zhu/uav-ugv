    <joint name='back_right_joint' type='revolute'>
      <!-- 恢复原始位姿：-0.08 -0.0845 0.04204（未修改z轴） -->
      <pose relative_to='base_footprint'>-0.08 -0.0845 0.04204 -1.57 -0 3.14159</pose>
      <parent>base_footprint</parent>
      <child>back_right_wheel</child>
      <axis>
        <xyz>0 0 1</xyz>
        <limit>
          <effort>100</effort>
          <velocity>1</velocity>
          <lower>-1e+16</lower>
          <upper>1e+16</upper>
