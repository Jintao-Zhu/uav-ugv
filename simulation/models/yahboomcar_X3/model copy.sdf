      <pose relative_to='back_right_joint'>0 0 0 0 -0 0</pose>  <!-- 原始位姿不变 -->
      <inertial>
        <pose>-2e-06 1e-06 -0.000591 0 -0 0</pose>
        <mass>0.0515402</mass>
        <inertia>
          <ixx>1.45974e-05</ixx>
          <ixy>1.94851e-10</ixy>
          <ixz>-1.61846e-09</ixz>
          <iyy>1.45965e-05</iyy>
          <iyz>-6.27904e-10</iyz>
          <izz>2.42663e-05</izz>
        </inertia>
      </inertial>
      <collision name='back_right_wheel_collision'>
        <pose>0 0 0 0 -0 0</pose>
        <geometry>
          <mesh>
            <scale>1 1 1</scale>
            <uri>model://yahboomcar_X3/meshes/back_right_wheel_X3.STL</uri>
