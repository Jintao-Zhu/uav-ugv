          <diffuse>0.7 0.7 0.7 1</diffuse>
        </material>
      </visual>
    </link>

    <!-- 右后轮（驱动轮） -->
    <joint name='back_right_joint' type='revolute'>
      <pose relative_to='base_footprint'>-0.08 -0.0845 0.042 -1.5708 0 3.14159</pose>
