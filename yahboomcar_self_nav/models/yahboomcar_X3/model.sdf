<?xml version="1.0" ?> 
<!--无人车配置 - 改进的四轮驱动实现--> 
<sdf version='1.7'>
  <model name='yahboomcar'>
  
    <static>false</static>
    <allow_auto_disable>true</allow_auto_disable>

    <link name='base_footprint'>
      <inertial>
        <pose>0.00687 0.000117 0.082436 0 -0 0</pose>
        <mass>0.53461</mass>
        <inertia>
          <ixx>0.00289164</ixx>
          <ixy>-7.86213e-07</ixy>
          <ixz>8.08187e-06</ixz>
          <iyy>0.00554364</iyy>
          <iyz>-3.63694e-06</iyz>
          <izz>0.00500707</izz>
        </inertia>
      </inertial>

      <self_collide>false</self_collide>
      <kinematic>true</kinematic>
      
      <!-- 底盘碰撞 -->
